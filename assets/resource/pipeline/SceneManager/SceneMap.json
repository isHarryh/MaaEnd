    "__ScenePrivateMapAnyEnterMapOverviewSuccess": {
        "doc": "在任意地图界面，成功进入地区总览界面",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -350,
                    -150,
                    350,
                    150
                ],
                "template": [
                    "SceneManager/MapOverviewValleyIVChoose.png",
                    "SceneManager/MapOverviewValleyIVNotChoose.png"
                ],
                "threshold": 0.9
            }
        }
    },
    "__ScenePrivateMapFilterClear": {
        "desc": "在地图界面，清除筛选条件",
        "recognition": "TemplateMatch",
        "roi": [
            0,
            -120,
            220,
            120
        ],
        "template": "SceneManager/MapFilterNotChoose.png",
        "action": "Click",
        "post_wait_freezes": 200,
        "next": [
            "__ScenePrivateMapFilterClearAll"
        ]
    },
    "__ScenePrivateMapFilterClearAll": {
        "desc": "在标记显示管理界面，点击清空选中",
        "recognition": "OCR",
        "roi": [
            0,
            -120,
            220,
            120
        ],
        "expected": [
            "清空选中",
            "クリア選択",
            "Clear Selection",
            "선택 해제"
        ],
        "action": {
            "type": "Click"
        },
        "post_wait_freezes": 200,
        "next": [
            "__ScenePrivateMapFilterClose"
        ]
    },
    "__ScenePrivateMapFilterClose": {
        "desc": "在标记显示管理界面，点击关闭",
        "recognition": "OCR",
        "roi": [
            0,
            0,
            200,
            80
