        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -200,
                    0,
                    200,
                    200
                ],
                "template": [
                    "SceneManager/WorldMenu.png"
                ],
                "green_mask": true
            }
        },
        "next": [
