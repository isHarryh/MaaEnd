    "__ScenePrivateMapDijiangEnterTeleport": {
        "doc": "在帝江号地图界面，随机选择一个传送点进入",
        "recognition": {
            "type": "And",
            "param": {
                "all_of": [
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "roi": [
                                    -350,
                                    0,
                                    350,
                                    60
                                ],
                                "template": [
                                    "SceneManager/MapMind.png"
                                ]
                            }
                        }
                    },
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "roi": [
                                    0,
                                    0,
                                    0,
                                    0
                                ],
                                "template": [
                                    "SceneManager/MapTeleport.png"
                                ]
                            }
                        }
                    },
                    {
                        "recognition": {
                            "type": "OCR",
                            "param": {
                                "roi": [
                                    0,
                                    0,
                                    350,
                                    220
                                ],
                                "expected": [
                                    "[Oo].?[Mm].?[Vv]"
                                ]
                            }
                        }
                    }
                ],
                "box_index": 1
            }
        },
        "action": {
            "type": "Click"
        },
        "post_wait_freezes": {
            "time": 400,
            "target": [
                -100,
                300,
                100,
                300
            ]
        },
        "next": [
            "[JumpBack]__ScenePrivateMapTeleportChoose",
            "__ScenePrivateMapTeleportConfirm"
        ]
    },
