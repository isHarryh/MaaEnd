            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -400,
                    -200,
                    400,
                    200
                ],
                "template": [
                    "SceneManager/MapOverviewEnter.png"
                ],
                "green_mask": true
            }
        },
        "action": {
            "type": "Click"
        },
        "post_wait_freezes": 400,
        "next": [
