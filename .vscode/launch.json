{
    "configurations": [
        {
            "name": "launch-go-agent",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/agent/go-service",
            "args": [
                "{AGENT_ID}"
            ],
            "cwd": "${workspaceFolder}/install"
        },
        {
            "name": "launch-cpp-agent",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/install/agent/cpp-algo",
            "args": [
                "{AGENT_ID}"
            ],
            "cwd": "${workspaceFolder}/install",
            "windows": {
                "env": {
                    "PATH": "${workspaceFolder}/install/maafw;${env:PATH}"
                }
            },
            "linux": {
                "env": {
                    "PATH": "${workspaceFolder}/install/maafw:${env:PATH}"
                }
            },
            "osx": {
                "env": {
                    "PATH": "${workspaceFolder}/install/maafw:${env:PATH}"
                }
            }
        }
    ]
}
