{
    "task": [
        {
            "name": "VisitFriends",
            "label": "$task.VisitFriends.label",
            "entry": "FriendVisitMain",
            "description": "$task.VisitFriends.description",
            "option": [
                "PriorStealVegetables"
            ],
            "controller": [
                "Win32",
                "Win32-Window",
                "Win32-Background",
                "Win32-Window-Background",
                "Win32-Front"
            ]
        }
    ],
    "option": {
        "PriorStealVegetables": {
            "type": "switch",
            "label": "$option.PriorStealVegetables.label",
            "description": "$option.PriorStealVegetables.description",
            "default_case": "No",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "PriorStealGateProductionAndClueCheck": {
                            "enabled": true
                        },
                        "ProductionAssistOCR": {
                            "enabled": false
                        },
                        "ClueExchangeOCR": {
                            "enabled": false
                        },
                        "ProductionAssistCheckPriorSteal": {
                            "enabled": true
                        },
                        "ProductionAssistCheck": {
                            "enabled": false
                        },
                        "GrowthChamberCheck": {
                            "enabled": true
                        },
                        "SkipConfirmBackToFriendList": {
                            "enabled": true
                        },
                        "WaitOpenTerminalAssistStealVeg": {
                            "enabled": true
                        },
                        "WaitOpenTerminalAssist": {
                            "enabled": false
                        },
                        "VisitorTerminalClueExchangeFirst": {
                            "enabled": true
                        },
                        "VisitorTerminalSwipeFacilityList": {
                            "enabled": true
                        },
                        "VisitorTerminalAssistGrowthChamber": {
                            "enabled": true
                        },
                        "VisitorTerminalCloseSeedDialog": {
                            "enabled": true
                        },
                        "VisitorTerminalAssist": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "PriorStealGateProductionAndClueCheck": {
                            "enabled": false
                        },
                        "ProductionAssistCheckPriorSteal": {
                            "enabled": false
                        },
                        "ProductionAssistOCR": {
                            "enabled": true
                        },
                        "ClueExchangeOCR": {
                            "enabled": true
                        },
                        "ProductionAssistCheck": {
                            "enabled": true
                        },
                        "GrowthChamberCheck": {
                            "enabled": false
                        },
                        "SkipConfirmBackToFriendList": {
                            "enabled": false
                        },
                        "WaitOpenTerminalAssistStealVeg": {
                            "enabled": false
                        },
                        "WaitOpenTerminalAssist": {
                            "enabled": true
                        },
                        "VisitorTerminalClueExchangeFirst": {
                            "enabled": false
                        },
                        "VisitorTerminalSwipeFacilityList": {
                            "enabled": false
                        },
                        "VisitorTerminalAssistGrowthChamber": {
                            "enabled": false
                        },
                        "VisitorTerminalCloseSeedDialog": {
                            "enabled": false
                        },
                        "VisitorTerminalAssist": {
                            "enabled": true
                        }
                    }
                }
            ]
        }
    }
}
