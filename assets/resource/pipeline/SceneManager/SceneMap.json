            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -250,
                    0,
                    250,
                    160
                ],
                "template": [
                    "SceneManager/MapExitDijiang.png"
                ]
            }
        },
        "action": {
            "type": "Click"
        },
        "post_wait_freezes": 400,
        "next": [
            "__ScenePrivateMapAnyEnterMapOverviewSuccess"
        ]
